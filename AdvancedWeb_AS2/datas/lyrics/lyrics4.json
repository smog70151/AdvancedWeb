lyrics = '[{"start_time":"800","d":"2940","text":"(In the living room)"},{"start_time":"6400","d":"2420","text":"Is that your mother’s picture?"},{"start_time":"9100","d":"1480","text":"Yes, it is."},{"start_time":"10700","d":"3020","text":"My mom is a police officer."},{"start_time":"14000","d":"960","text":"Wow!"},{"start_time":"15080","d":"3320","text":"Is your father a police officer, too?"},{"start_time":"18500","d":"1780","text":"No, he isn’t."},{"start_time":"20360","d":"2300","text":"He is a nurse."},{"start_time":"22700","d":"2800","text":"Cool!"}]'
