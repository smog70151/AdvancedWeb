lyrics = '[{"start_time":"1960","d":"2335","text":"YOU SHOULD DRINK FROM THIS ENCHANTED SPRING."},{"start_time":"4295","d":"2962","text":"IT MIGHT GIVE YOU CRAZY DREAMS, BUT WHEN YOU WAKE UP"},{"start_time":"7257","d":"2461","text":"YOU\'LL BE IMMUNE TO THE BOAR\'S ELECTRICAL ATTACKS."},{"start_time":"9718","d":"2377","text":"LET\'S ROLL THOSE DICE. [ GULPING ]"},{"start_time":"12095","d":"2002","text":"HEY, DUDE, ARE YOU SURE YOU WANT TO DO THAT?"},{"start_time":"14097","d":"1251","text":"MM-HMM. BLAH!"},{"start_time":"15348","d":"2670","text":"I\'VE HAD PLENTY OF WEIRD DREAMS BEFO-- OKAY!"},{"start_time":"18018","d":"3086","text":"THAT WAS FAST. [ CHIRPS ]"},{"start_time":"23273","d":"4129","text":"WELCOME. I AM THE SPIRIT OF THE FOREST."},{"start_time":"27944","d":"3295","text":"WOW, DUDE, WE\'VE BEEN TRYING TO SUMMON YOU FOR WEEKS."},{"start_time":"31239","d":"1710","text":"AND WHY DO YOU SEEK ME?"},{"start_time":"32949","d":"1502","text":"I DON\'T, PERSONALLY,"},{"start_time":"34451","d":"2002","text":"BUT HUNTRESS WIZARD REALLY WANTS TO TALK TO YOU, MAN."},{"start_time":"36453","d":"3545","text":"DID YOU KNOW THAT A WOLF\'S EYES ARE ON THE FRONT OF ITS HEAD?"},{"start_time":"39998","d":"2085","text":"BUT, YEAH, DIDN\'T YOU GUYS USED TO HANG?"},{"start_time":"42083","d":"2294","text":"MOST PREDATORY SPECIES ARE LIKE THAT."},{"start_time":"44377","d":"1960","text":"THIS LITTLE BUDDY DOESN\'T EVEN KNOW I\'M HERE."},{"start_time":"46337","d":"2545","text":"FELLA, I DON\'T WANT TO HAVE A SCIENCE TALK RIGHT NOW."},{"start_time":"48882","d":"2836","text":"HUNTRESS WIZARD ONLY SEES STRAIGHT AHEAD."},{"start_time":"51718","d":"1501","text":"EYES ON THE PRIZE."},{"start_time":"53219","d":"2127","text":"NOW, WHY DID YOU REALLY NEED TO SEE ME?"},{"start_time":"55346","d":"3212","text":"OH, YEAH! CAN YOU MAKE ME IMMUNE TO ELECTRICITY?"},{"start_time":"58558","d":"1460","text":"HERE. WHOA."},{"start_time":"60018","d":"2210","text":"NOW PLUNGE THAT INTO YOUR HEART."},{"start_time":"62228","d":"3754","text":"AND THIS\'LL GIVE ME IMMUNITY TO ELECTRICITY?"},{"start_time":"66941","d":"3671","text":"I\'LL DO IT... TO HELP HUNTRESS WIZARD."},{"start_time":"71905","d":"1293","text":"AAH!"},{"start_time":"73198","d":"1042","text":"HOW LONG WAS I OUT?"},{"start_time":"74240","d":"2211","text":"SEVEN LONG, HARROWING SECONDS."},{"start_time":"76451","d":"1335","text":"HOLD STILL."},{"start_time":"77786","d":"1209","text":"[ ELECTRICITY CRACKLES ]"},{"start_time":"78995","d":"1794","text":"YEAH, FEELS FINE. GREAT."}]'
