data0 = '[{"start_time":"900","d":"935","text":"Teacher: Shh, shh. Settle, settle, please."},{"start_time":"1868","d":"3003","text":"Eyes up here, eyes up here. We`re not done."},{"start_time":"4904","d":"5606","text":"Group three, let`s have Ivy, and Michael,"},{"start_time":"10543","d":"3036","text":"uh, Ryan, Sally,"},{"start_time":"13612","d":"2403","text":"and... Thomas."},{"start_time":"16048","d":"1335","text":"Thomas?"},{"start_time":"17416","d":"5606","text":"Your homework is to explore [THUD] gravity."},{"start_time":"23055","d":"2003","text":"[BELL RINGING] Don`t forget,"},{"start_time":"25091","d":"1802","text":"projects are due on Friday."},{"start_time":"26926","d":"2102","text":"Friday, please! Friday."},{"start_time":"29061","d":"1502","text":"And don`t forget your..."},{"start_time":"30596","d":"1368","text":"[INDISTINCT CHATTER] ...homework."},{"start_time":"31997","d":"1536","text":"♪ Music playing ♪"},{"start_time":"33566","d":"4438","text":"Voiceover: Homework. Oh, homework."},{"start_time":"38037","d":"2069","text":"I hate you."},{"start_time":"40139","d":"2002","text":"You stink."},{"start_time":"42174","d":"2270","text":"I wish I could wash you away in the sink."},{"start_time":"44477","d":"2336","text":"Child: You guys ready? Children: Almost ready!"},{"start_time":"46846","d":"4204","text":"Voiceover: If only a hippo would smash you to bits."},{"start_time":"51083","d":"2670","text":"Homework. Oh, homework."},{"start_time":"53786","d":"1068","text":"Child: Did we get it?"},{"start_time":"54887","d":"1535","text":"Voiceover: Your giving me fits."},{"start_time":"56455","d":"3971","text":"Children: Wow! We got it! That`s amazing! Yeah!"},{"start_time":"60459","d":"3504","text":"Voiceover: I`d rather take baths with a man-eating shark,"},{"start_time":"63996","d":"3337","text":"or wrestle a lion alone in the dark,"},{"start_time":"67366","d":"2670","text":"eat spinach and liver,"},{"start_time":"70069","d":"2503","text":"pet 10 porcupines,"},{"start_time":"72605","d":"3904","text":"than tackle the homework my teacher assigns."},{"start_time":"76542","d":"1268","text":"Child: Action."},{"start_time":"77843","d":"8743","text":"♪"},{"start_time":"86619","d":"1134","text":"Child: Dad!"},{"start_time":"87786","d":"2036","text":"Voiceover: Homework."},{"start_time":"89855","d":"3004","text":"Oh, homework."},{"start_time":"92892","d":"2002","text":"You`re last on my list."},{"start_time":"94927","d":"2570","text":"[CHILDREN SHOUTING]"},{"start_time":"97530","d":"3003","text":"I simply can`t see why you even exist."},{"start_time":"100566","d":"13280","text":"♪"},{"start_time":"113879","d":"2370","text":"[BELL RINGING] If you just disappeared,"},{"start_time":"116282","d":"2436","text":"it would tickle me pink."},{"start_time":"118751","d":"2569","text":"Teacher: Next up, group three."},{"start_time":"121353","d":"3471","text":"Voiceover: Homework. Oh, homework."},{"start_time":"124857","d":"1335","text":"I hate you."},{"start_time":"126225","d":"2236","text":"♪"},{"start_time":"128494","text":"You stink."}]'
data4 = '[{"start_time":"800","d":"2940","text":"(In the living room)"},{"start_time":"6400","d":"2420","text":"Is that your mother’s picture?"},{"start_time":"9100","d":"1480","text":"Yes, it is."},{"start_time":"10700","d":"3020","text":"My mom is a police officer."},{"start_time":"14000","d":"960","text":"Wow!"},{"start_time":"15080","d":"3320","text":"Is your father a police officer, too?"},{"start_time":"18500","d":"1780","text":"No, he isn’t."},{"start_time":"20360","d":"2300","text":"He is a nurse."},{"start_time":"22700","d":"2800","text":"Cool!"}]'
data3 = '[{"start_time":"1960","d":"2335","text":"YOU SHOULD DRINK FROM THIS ENCHANTED SPRING."},{"start_time":"4295","d":"2962","text":"IT MIGHT GIVE YOU CRAZY DREAMS, BUT WHEN YOU WAKE UP"},{"start_time":"7257","d":"2461","text":"YOU\'LL BE IMMUNE TO THE BOAR\'S ELECTRICAL ATTACKS."},{"start_time":"9718","d":"2377","text":"LET\'S ROLL THOSE DICE. [ GULPING ]"},{"start_time":"12095","d":"2002","text":"HEY, DUDE, ARE YOU SURE YOU WANT TO DO THAT?"},{"start_time":"14097","d":"1251","text":"MM-HMM. BLAH!"},{"start_time":"15348","d":"2670","text":"I\'VE HAD PLENTY OF WEIRD DREAMS BEFO-- OKAY!"},{"start_time":"18018","d":"3086","text":"THAT WAS FAST. [ CHIRPS ]"},{"start_time":"23273","d":"4129","text":"WELCOME. I AM THE SPIRIT OF THE FOREST."},{"start_time":"27944","d":"3295","text":"WOW, DUDE, WE\'VE BEEN TRYING TO SUMMON YOU FOR WEEKS."},{"start_time":"31239","d":"1710","text":"AND WHY DO YOU SEEK ME?"},{"start_time":"32949","d":"1502","text":"I DON\'T, PERSONALLY,"},{"start_time":"34451","d":"2002","text":"BUT HUNTRESS WIZARD REALLY WANTS TO TALK TO YOU, MAN."},{"start_time":"36453","d":"3545","text":"DID YOU KNOW THAT A WOLF\'S EYES ARE ON THE FRONT OF ITS HEAD?"},{"start_time":"39998","d":"2085","text":"BUT, YEAH, DIDN\'T YOU GUYS USED TO HANG?"},{"start_time":"42083","d":"2294","text":"MOST PREDATORY SPECIES ARE LIKE THAT."},{"start_time":"44377","d":"1960","text":"THIS LITTLE BUDDY DOESN\'T EVEN KNOW I\'M HERE."},{"start_time":"46337","d":"2545","text":"FELLA, I DON\'T WANT TO HAVE A SCIENCE TALK RIGHT NOW."},{"start_time":"48882","d":"2836","text":"HUNTRESS WIZARD ONLY SEES STRAIGHT AHEAD."},{"start_time":"51718","d":"1501","text":"EYES ON THE PRIZE."},{"start_time":"53219","d":"2127","text":"NOW, WHY DID YOU REALLY NEED TO SEE ME?"},{"start_time":"55346","d":"3212","text":"OH, YEAH! CAN YOU MAKE ME IMMUNE TO ELECTRICITY?"},{"start_time":"58558","d":"1460","text":"HERE. WHOA."},{"start_time":"60018","d":"2210","text":"NOW PLUNGE THAT INTO YOUR HEART."},{"start_time":"62228","d":"3754","text":"AND THIS\'LL GIVE ME IMMUNITY TO ELECTRICITY?"},{"start_time":"66941","d":"3671","text":"I\'LL DO IT... TO HELP HUNTRESS WIZARD."},{"start_time":"71905","d":"1293","text":"AAH!"},{"start_time":"73198","d":"1042","text":"HOW LONG WAS I OUT?"},{"start_time":"74240","d":"2211","text":"SEVEN LONG, HARROWING SECONDS."},{"start_time":"76451","d":"1335","text":"HOLD STILL."},{"start_time":"77786","d":"1209","text":"[ ELECTRICITY CRACKLES ]"},{"start_time":"78995","d":"1794","text":"YEAH, FEELS FINE. GREAT."}]'
data2 = '[{"start_time":"1219","d":"4661","text":"I wanna make a case for rethinking how we run our businesses."},{"start_time":"5880","d":"1340","text":"Take a look at this."},{"start_time":"7220","d":"3200","text":"This is called the candle problem and here\'s how it works."},{"start_time":"10420","d":"6750","text":"I bring you into a room, I give you a candle, some thumbtacks, and some matches and I say"},{"start_time":"17170","d":"7099","text":"to you: Your job is to attach the candle to the wall so the wax doesn\'t drip on the table."},{"start_time":"24269","d":"4480","text":"And eventually after 5 or 10 minutes, most people figure out the solution which you can"},{"start_time":"28749","d":"1000","text":"see here."},{"start_time":"29749","d":"3801","text":"The key is to overcome what is called functional fixedness."},{"start_time":"33550","d":"4500","text":"You look at that box and you see it only as a receptacle for the tacks, but it can also"},{"start_time":"38050","d":"3170","text":"have this other function as a platform for the candle."},{"start_time":"41220","d":"5450","text":"Now, I wanna tell you about an experiment using the candle problem, done by a scientist"},{"start_time":"46670","d":"4320","text":"named Sam Glucksberg - he gathered his participants and he says I\'m gonna time you, how quickly"},{"start_time":"50990","d":"1540","text":"you can solve this problem."},{"start_time":"52530","d":"7850","text":"He offered rewards, he said if you\'re in the top 25% of the fastest times, you get 5 dollars."},{"start_time":"60380","d":"5170","text":"If you\'re the fastest of everyone we\'re testing here today, you get 20 dollars."},{"start_time":"65550","d":"4600","text":"How much faster did this group solve the problem?"},{"start_time":"70150","d":"6350","text":"Answer: It took them on average 3 and a half minutes... longer."},{"start_time":"76500","d":"3520","text":"Three and a half minutes longer, now this makes no sense, right?"},{"start_time":"80020","d":"4060","text":"If you want people to perform better, you reward them, right?"},{"start_time":"84080","d":"4829","text":"Bonuses, commissions, their own reality show, incentivize them."},{"start_time":"88909","d":"5651","text":"You\'ve got an incentive designed to sharpen thinking and accelerate creativity and it"},{"start_time":"94560","d":"1860","text":"does just the opposite."},{"start_time":"96420","d":"6200","text":"These contingent motivators: If you do this then you get that work in some circumstances"},{"start_time":"102620","d":"6470","text":"but for a lot of tasks, they actually either don\'t work or often they do harm."},{"start_time":"109090","d":"4709","text":"Think about your own work: Are the problems that you face or even the problems we\'ve been"},{"start_time":"113799","d":"4601","text":"talking about here, are those kinds of problems - do they have a clear set of rules and a"},{"start_time":"118400","d":"1510","text":"single solution?"},{"start_time":"119910","d":"1010","text":"No!"},{"start_time":"120920","d":"4800","text":"Those if - then rewards DON\'T WORK!"},{"start_time":"125720","d":"4870","text":"Economists at LSE looked at 51 studies of pay for performance plans inside of companies."},{"start_time":"130590","d":"3720","text":"Here\'s what the economists there said: We find that financial incentives can result"},{"start_time":"134310","d":"4120","text":"in a negative impact on overall performance."},{"start_time":"138430","d":"5100","text":"There\'s a mismatch between what science knows and what business does."},{"start_time":"143530","d":"1420","text":"Here\'s what science knows: 1."},{"start_time":"144950","d":"5090","text":"Those 20th century rewards, those motivators we think are the natural part of business"},{"start_time":"150040","d":"3520","text":"do work but only in a surprisingly narrow band of circumstances."},{"start_time":"153560","d":"1009","text":"2."},{"start_time":"154569","d":"2941","text":"Those if then rewards often destroy creativity."},{"start_time":"157510","d":"1000","text":"3."},{"start_time":"158510","d":"4090","text":"The secret to high performance isn\'t rewards and punishments but that unseen intrinsic"},{"start_time":"162600","d":"4220","text":"drive, the drive to do things for their own sake, the drive to do things because they"},{"start_time":"166820","d":"1000","text":"matter."},{"start_time":"167820","d":"4","text":"I rest my case."}]'
data1 ='[{"start_time":"200","d":"1820","text":"♪ Let me out! ♪"},{"start_time":"3240","d":"1960","text":"♪ I\'m free to run, ♪"},{"start_time":"5700","d":"2860","text":"♪ Rip these chains and its done. ♪"},{"start_time":"8980","d":"2020","text":"♪ Let me out! ♪"},{"start_time":"11920","d":"2080","text":"♪ I\'m free to run! ♪"},{"start_time":"14480","d":"3120","text":"♪ Break the ruses for fun. ♪"},{"start_time":"18520","d":"1980","text":"♪ I know who I am ♪"},{"start_time":"20700","d":"2100","text":"♪ and how it all began. ♪"},{"start_time":"22980","d":"1740","text":"♪ It started this way! ♪"},{"start_time":"24720","d":"2240","text":"♪ Let me go-o man! ♪"},{"start_time":"27200","d":"1960","text":"♪ So split this parade, ♪"},{"start_time":"29460","d":"2160","text":"♪ and no more sharade, ♪"},{"start_time":"31920","d":"3720","text":"♪ it\'s time that we bounce this back in! ♪"},{"start_time":"35640","d":"1320","text":"♪ Let me out! ♪"},{"start_time":"38460","d":"1840","text":"♪ Free to run! ♪"},{"start_time":"40640","d":"3360","text":"♪ Rip these chains and it\'s done. ♪"},{"start_time":"44480","d":"1540","text":"♪ Let me out! ♪"},{"start_time":"47160","d":"1760","text":"♪ Free to run! ♪"},{"start_time":"49360","d":"1860","text":"♪ Throw some shade. ♪"},{"start_time":"51660","d":"3660","text":"♪ Quit right now and get paid. ♪"},{"start_time":"55320","d":"2300","text":"♪♪"}]'


