lyrics = '[{"start_time":"900","d":"935","text":"Teacher: Shh, shh. Settle, settle, please."},{"start_time":"1868","d":"3003","text":"Eyes up here, eyes up here. We`re not done."},{"start_time":"4904","d":"5606","text":"Group three, let`s have Ivy, and Michael,"},{"start_time":"10543","d":"3036","text":"uh, Ryan, Sally,"},{"start_time":"13612","d":"2403","text":"and... Thomas."},{"start_time":"16048","d":"1335","text":"Thomas?"},{"start_time":"17416","d":"5606","text":"Your homework is to explore [THUD] gravity."},{"start_time":"23055","d":"2003","text":"[BELL RINGING] Don`t forget,"},{"start_time":"25091","d":"1802","text":"projects are due on Friday."},{"start_time":"26926","d":"2102","text":"Friday, please! Friday."},{"start_time":"29061","d":"1502","text":"And don`t forget your..."},{"start_time":"30596","d":"1368","text":"[INDISTINCT CHATTER] ...homework."},{"start_time":"31997","d":"1536","text":"♪ Music playing ♪"},{"start_time":"33566","d":"4438","text":"Voiceover: Homework. Oh, homework."},{"start_time":"38037","d":"2069","text":"I hate you."},{"start_time":"40139","d":"2002","text":"You stink."},{"start_time":"42174","d":"2270","text":"I wish I could wash you away in the sink."},{"start_time":"44477","d":"2336","text":"Child: You guys ready? Children: Almost ready!"},{"start_time":"46846","d":"4204","text":"Voiceover: If only a hippo would smash you to bits."},{"start_time":"51083","d":"2670","text":"Homework. Oh, homework."},{"start_time":"53786","d":"1068","text":"Child: Did we get it?"},{"start_time":"54887","d":"1535","text":"Voiceover: Your giving me fits."},{"start_time":"56455","d":"3971","text":"Children: Wow! We got it! That`s amazing! Yeah!"},{"start_time":"60459","d":"3504","text":"Voiceover: I`d rather take baths with a man-eating shark,"},{"start_time":"63996","d":"3337","text":"or wrestle a lion alone in the dark,"},{"start_time":"67366","d":"2670","text":"eat spinach and liver,"},{"start_time":"70069","d":"2503","text":"pet 10 porcupines,"},{"start_time":"72605","d":"3904","text":"than tackle the homework my teacher assigns."},{"start_time":"76542","d":"1268","text":"Child: Action."},{"start_time":"77843","d":"8743","text":"♪"},{"start_time":"86619","d":"1134","text":"Child: Dad!"},{"start_time":"87786","d":"2036","text":"Voiceover: Homework."},{"start_time":"89855","d":"3004","text":"Oh, homework."},{"start_time":"92892","d":"2002","text":"You`re last on my list."},{"start_time":"94927","d":"2570","text":"[CHILDREN SHOUTING]"},{"start_time":"97530","d":"3003","text":"I simply can`t see why you even exist."},{"start_time":"100566","d":"13280","text":"♪"},{"start_time":"113879","d":"2370","text":"[BELL RINGING] If you just disappeared,"},{"start_time":"116282","d":"2436","text":"it would tickle me pink."},{"start_time":"118751","d":"2569","text":"Teacher: Next up, group three."},{"start_time":"121353","d":"3471","text":"Voiceover: Homework. Oh, homework."},{"start_time":"124857","d":"1335","text":"I hate you."},{"start_time":"126225","d":"2236","text":"♪"},{"start_time":"128494","text":"You stink."}]'
